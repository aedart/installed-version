{
  "name": "aedart/installed-version",
  "description": "Utility to identify what package version you have installed",
  "keywords": [
    "Version",
    "Composer",
    "Package"
  ],
  "homepage": "https://github.com/aedart/installed-version",
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Alin Eugen Deac",
      "email": "aedart@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "composer/composer": "~1.2"
  },
  "require-dev": {
    "aedart/license": "1.*",
    "aedart/license-file-manager": "~1.6",
    "aedart/testing": "~1.0",
    "aedart/testing-gst": "~1.2"
  },
  "autoload": {
    "psr-4": {
      "Aedart\\Installed\\Version\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "post-update-cmd": "vendor/bin/license-manager license:copy vendor/aedart/license/aedart/BSD-3-Clause"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
